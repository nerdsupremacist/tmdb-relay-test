{
  "name": "tmdb-relay-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.12",
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.11",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "framer-motion": "^4.1.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-hotkeys-hook": "^3.3.1",
    "react-parallax": "^3.3.0",
    "react-relay": "^11.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "relay-config": "^11.0.2",
    "relay-runtime": "^11.0.2",
    "typescript": "^4.2.4",
    "use-debounce": "^6.0.1",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "npm run relay && react-scripts start",
    "build": "npm run relay && react-scripts build",
    "test": "npm run relay && react-scripts test",
    "eject": "npm run relay && react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./data/schema.graphql --extensions ts tsx --language typescript --customScalars.URL=string --customScalars.Date=string --watchman false $@",
    "validate-relay": "relay-compiler --validate  --src ./src --schema ./data/schema.graphql --extensions ts tsx --language typescript --customScalars.URL=string --customScalars.Date=string",
    "lint": "eslint . --ignore-pattern node_modules/ --ext .js,.jsx,.ts,.tsx",
    "autofix": "eslint . --ignore-pattern node_modules/ --ext .js,.jsx,.ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-relay": "^11.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-relay": "^11.0.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-relay": "^1.8.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "graphql": "^15.5.0",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.0.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "relay",
      "macros"
    ]
  }
}